{
  "name": "terraria-web-editor",
  "version": "1.0.0",
  "description": "Terraria map editor, in a web browser",
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "core-js": "^3.9.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@mischnic/parcel-resolver-root": "^0.2.1",
    "@parcel/optimizer-cssnano": "^2.0.0-nightly.602",
    "@parcel/optimizer-htmlnano": "^2.0.0-nightly.602",
    "@parcel/packager-css": "^2.0.0-nightly.602",
    "@parcel/packager-html": "^2.0.0-nightly.602",
    "@parcel/transformer-css": "^2.0.0-nightly.602",
    "@parcel/transformer-html": "^2.0.0-nightly.602",
    "@parcel/transformer-image": "^2.0.0-nightly.2224",
    "@parcel/transformer-postcss": "^2.0.0-nightly.602",
    "@parcel/transformer-posthtml": "^2.0.0-nightly.602",
    "cross-env": "^6.0.3",
    "parcel": "^2.0.0-nightly.600",
    "postcss": "^8.2.6"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development    parcel       ./src/index.html --port 80 --no-hmr",
    "build": "cross-env NODE_ENV=production     parcel build ./src/index.html --dist-dir dist/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "@mischnic/parcel-resolver-root": {
    "/": "./src",
    "~": "./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cokolele/terraria-web-editor.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cokolele/terraria-web-editor/issues"
  },
  "homepage": "https://github.com/cokolele/terraria-web-editor#readme"
}
